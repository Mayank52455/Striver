class Node:
    def __init__(self, data, next=None):
        self.data = data
        self.next = next

class Solution:
    def con_ll(self, arr):
        if not arr:      # if array is empty
            return None

        head = Node(arr[0])   # first node
        current = head

        for val in arr[1:]:   # loop through remaining elements
            current.next = Node(val)  # create new node
            current = current.next    # move forward

        return head

def print_ll(head):
    current = head
    while current:
        print(current.data, end="->" if current.next else "\n")
        current = current.next
